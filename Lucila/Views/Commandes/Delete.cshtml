@model ProjetFinalMVC.DAL.Commandes
@{
    ViewBag.Title = "Delete";
}


<h2>Delete</h2>
<style>
    table {
        border-collapse: collapse;
        width: 100%;
    }

    th, td {
        padding: 8px;
        text-align: left;
        border-bottom: 1px solid #DDD;
    }

    tr:hover {
        background-color: #D6EEEE;
    }
</style>

<table>
    <tr>
        <th>id</th>
        <th>idclient</th>
        <th>date</th>
        <th>detail</th>
        <th>prixtotal</th>
    </tr>

    <tr>
        <td>@Model.Id</td>
        <td>@Model.IdClient</td>
        <td>@Model.Date</td>
        <td>@Model.Detail</td>
        <td>@Model.PrixTotal</td>
    </tr>


</table>
